          object cmbRunCmd: TComboBox
            Left = 0
            Height = 31
            Hint = 'list of variables:'#13#10'$ffmpeg'#13#10'$ffplay'#13#10'$ffprobe'#13#10'$input'#13#10'$output'#13#10'$dirinp'#13#10'$dirout'#13#10'$dirtmp'
            Top = 0
            Width = 940
            Align = alTop
            DropDownCount = 20
            ItemHeight = 0
            ItemIndex = 16
            Items.Strings = (
              '"$ffplay" -i "$input"'
              '"$ffplay" -f lavfi ''amovie=$input, asplit [a][out1]; [a] showcqt=s=1280x720 [out0]'' -autoexit'
              '"$ffplay" -f lavfi ''amovie=$input, asplit [a][out1]; [a] showfreqs=s=1280x720 [out0]'' -autoexit'
              '"$ffplay" -f lavfi ''amovie=$input, asplit [a][out1]; [a] showvolume [out0]'' -autoexit'
              '"$ffplay" -f lavfi ''amovie=$input, asplit [a][out1]; [a] showwaves=s=1280x720 [out0]'' -autoexit'
              '"$ffprobe" -show_format -show_streams -i "$input"'
              '"$ffprobe" -show_format -show_streams -print_format ini -i "$input"'
              '"$ffprobe" -show_frames -i "$input"'
              '"$ffmpeg" -i "$input" -vsync 1 -r 1 -f image2 "$dirtmp/img-%03d.jpg"'
              '"$ffmpeg" -i "$input" -f image2 -frames:v 1 "$dirtmp/img.bmp"'
              '"$ffmpeg" -f image2 -i img%d.jpg "$dirtmp/a.mpg"'
              '"$ffmpeg" -i "$input" -vf "[in] split [T1], [T2] overlay=0:H/2 [out]; [T1] crop=iw:ih/2:0:ih/2, vflip [T2]" "$output"'
              '"$ffmpeg" -i "$dirinp/VTS_01_1.VOB" -i "$dirinp/VTS_01_2.VOB" -filter_complex "[0:0] [0:1] [1:0] [1:1] concat=n=2:v=1:a=1 [v] [a]" -map "[v]" -map "[a]" "$output"'
              '"$ffmpeg" -i "$input" -dump_attachment:t:0 "$dirout/attach-0"'
              '"$ffmpeg" -encoders'
              '"$ffmpeg" -formats'
              '"$ffmpeg" -muxers'
              '"$ffmpeg" -decoders'
              '"$ffmpeg" -devices'
              '"$ffmpeg" -sample_fmts'
              '"$ffmpeg" -filters'
              '"$ffmpeg" -protocols'
              '"$ffmpeg" -codecs'
              '"$ffmpeg" -bsfs'
              '"$ffprobe" -hide_banner -list_formats all -f v4l2 /dev/video0'
              '"$ffmpeg" -hide_banner -h encoder=h264_nvenc'
              '"$ffmpeg" -hide_banner -i "$input" -frames 20 -vf framestep=120,cropdetect -an -f null -y /dev/null'
              '"$ffmpeg" -i "$input" -i "$output" -lavfi "ssim;[0:v][1:v]psnr" -f null -'
              '"$ffmpeg" -i "$input" -vf "select=eq(n\,100)" -frames:v 1 "$dirout/frame000100.png"'
            )
            OnGetItems = cmbRunCmdGetItems
            TabOrder = 2
            Text = '"$ffmpeg" -muxers'
          end
